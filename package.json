{
    "name": "fruity-plugin-vscode",
    "displayName": "Fruity",
    "description": "Fruity language support",
    "version": "1.0.0",
    "publisher": "deadlyfugu",
    "repository": {
        "type": "git",
        "url": "https://github.com/DeadlyFugu/vscode-fruity"
    },
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "fruity",
            "aliases": ["Fruity", "fruity"],
            "extensions": [".fj"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "fruity",
            "scopeName": "source.fruity",
            "path": "./syntaxes/fruity.tmLanguage.json"
        }]
    }
}